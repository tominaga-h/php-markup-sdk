{
    "name": "hytmng/php-markup-sdk",
    "description": "A SDK for creating your own markup language",
    "version": "0.0.1",
    "type": "project",
    "license": "MIT",
    "keywords": [
    ],
    "authors": [
        {
            "name": "Tominaga Hayato"
        }
    ],
    "bin": [""],
    "autoload": {
        "psr-4": {
            "Hytmng\\MarkupSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "php-coveralls/php-coveralls": "^2.7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "upload-coverage": "./vendor/bin/php-coveralls --coverage_clover=coverage.xml -v"
    }
}
